<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.client.news.MainActivity">


    <Button
        android:id="@+id/click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="点击调起" />

    <com.client.news.logcat.LogcatTextView
        android:id="@+id/logcat"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:consoleColor="@color/defaultConsoleColor"
        app:debugColor="@color/defaultDebugColor"
        app:errorColor="@color/defaultErrorColor"
        app:infoColor="@color/defaultInfoColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:verboseColor="@color/defaultVerboseColor"
        app:warningColor="@color/defaultWarningColor" />


</android.support.constraint.ConstraintLayout>
